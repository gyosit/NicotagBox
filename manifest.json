{
    "name": "Nicotag-box",
    "version": "1.1.0",
    "manifest_version": 3,
    "description": "Transfer tag link to another link",
    "content_scripts": [{
      "matches": ["https://www.nicovideo.jp/*"],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }],
    "action": {
      "default_popup": "index.html"
    },
    "icons": {
      "128": "128.png"
    },
    "permissions": [
      "storage"
    ]
  }